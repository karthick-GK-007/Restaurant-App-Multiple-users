{
  "rewrites": [
    {
      "source": "/kagzso/admin/:hotel/:branch",
      "destination": "/admin.html"
    },
    {
      "source": "/kagzso/admin/:hotel",
      "destination": "/admin.html"
    },
    {
      "source": "/kagzso/user/:hotel/:branch",
      "destination": "/index.html"
    },
    {
      "source": "/kagzso/user/:hotel",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}

